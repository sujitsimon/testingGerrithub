[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/sujitsimon (sujitsimon)
[access "refs/for/refs/*"]
	read = group user/sujitsimon (sujitsimon)
	push = group user/sujitsimon (sujitsimon)
	label-Code-Review = -2..+2 group user/sujitsimon (sujitsimon)
	label-Verified = -1..+1 group user/sujitsimon (sujitsimon)
	rebase = group user/sujitsimon (sujitsimon)
	removeReviewer = group user/sujitsimon (sujitsimon)
	submit = group user/sujitsimon (sujitsimon)
[access "refs/heads/*"]
	read = group user/sujitsimon (sujitsimon)
	create = group user/sujitsimon (sujitsimon)
	push = +force group user/sujitsimon (sujitsimon)
	pushMerge = group user/sujitsimon (sujitsimon)
[access "refs/tags/*"]
	push = group user/sujitsimon (sujitsimon)
	push = +force group user/sujitsimon (sujitsimon)
[project]
	description = Testing Gerrithub
[receive]
	requireChangeId = true
